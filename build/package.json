    "lint:stylelint.report.verbose": "stylelint --config stylelint.config.cjs -f verbose -o reports/stylelintreport.verbose.txt Assets/Scss/**/*.scss",
    "test": "jest --watch",
    "st": "ddev start",
    "rst": "ddev restart",
    "stp": "ddev stop"
  },
