    "lint:eslint.report.junit": "eslint -f junit -o reports/eslintreport.junit.xml Assets/**/*.js",
    "lint:stylelint.report.json": "stylelint --config stylelint.config.cjs -f json -o reports/stylelintreport.json Assets/Scss/**/*.scss",
